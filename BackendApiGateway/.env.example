# Backend
LOG_LEVEL= # example value: "info"
BACKEND_API_PORT= #NOT for DB!
ACCESS_TOKEN_SECRET=
REFRESH_TOKEN_SECRET=

# audience is the url of the frontend app
# issuer is the url of the backend api gateway
API_GATEWAY_AUDIENCE=
API_GATEWAY_ISSUER=
ALLOWED_ORIGIN=

## DB
# Prisma database url
# this env var is loaded into prisma.config.ts
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=
DATABASE_URL=postgresql://<POSTGRES_USER>:<POSTGRES_PASSWORD>@<domain>/<POSTGRES_DB>?schema=public
